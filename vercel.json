{
    "crons": [
        {
            "path": "/api/todayTradingPrice",
            "schedule": "*/2 * * * *"
        }
    ],
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}